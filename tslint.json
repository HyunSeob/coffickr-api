{
    "extends": "tslint-microsoft-contrib",
    "rules": {
      "ordered-imports": false,
      "missing-jsdoc": false,
      "member-access": false,
      "no-relative-imports": false,
      "trailing-comma": "always",
      "typedef": false,
      "no-backbone-get-set-outside-model": false,
      "interface-name": false,
      "no-stateless-class": false
    }
}
